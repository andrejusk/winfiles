{
    "targets": [{
        "src": "./*.ps1",
        "dst": ".",
        "exclude": "bootstrap.ps1"
    }, {
        "src": "./components/**",
        "dst": "components",
        "wipe": true
    }, {
        "src": "./config/**",
        "dst": "config",
        "wipe": true
    }, {
        "src": "./config/windows/**",
        "dst": "config/windows",
        "wipe": true
    }, {
        "base": "home",
        "src": "./home/**",
        "dst": "."
    }, {
        "base": "appdata",
        "src": "./vscode/**",
        "dst": "Code/User"
    }]
}
